<?xml version="1.0" encoding="UTF-8"?>
<uwsgi>
    <socket>127.0.0.1:8000</socket> <!-- 内部端口，自定义 -->
    <chdir>/home/shopping/</chdir> <!-- 项目路径 -->
    <module>library.wsgi</module> <!-- wsgi.py所在目录名-->
    <processes>4</processes> <!-- 进程数 -->
    <daemonize>uwsgi.log</daemonize> <!-- 日志文件 -->
</uwsgi>
